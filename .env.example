# Spotify API Credentials
# Get these from https://developer.spotify.com/dashboard
SPOTIFY_CLIENT_ID=your_spotify_client_id_here
SPOTIFY_CLIENT_SECRET=your_spotify_client_secret_here

# Spotify Playlist ID
# Format: spotify:playlist:XXXXXXXXXXXXXXXXXXXXXX
# You can get this from the Spotify playlist URL
SPOTIFY_PLAYLIST_ID=spotify:playlist:your_playlist_id_here

# Navidrome Credentials
# Your Navidrome server credentials
NAVIDROME_USER=your_navidrome_username
NAVIDROME_PASS=your_navidrome_password

# Soulseek Network Credentials
# Your Soulseek username and password
SLSKD_SLSK_USERNAME=your_soulseek_username
SLSKD_SLSK_PASSWORD=your_soulseek_password

# Slskd API Key
# This should match the API key in slskd.yml
SLSKD_API_KEY=your_slskd_api_key_here
